datasource db {
    // Change provider to whichever provide you're using.
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

enum CommandType {
    TEXT_COMMAND
    APPLICATION_COMMAND
}

model Cooldown {
    commandName String
    commandType CommandType
    userId      String
    createdAt   DateTime    @default(now())

    @@id([commandName, commandType, userId])
    @@map("command_cooldowns")
}

model GuildConfig {
    guildId String @id

    language String?

    @@map("guild_configs")
}

model UserConfig {
    userId String @id

    language String?

    @@map("user_configs")
}
